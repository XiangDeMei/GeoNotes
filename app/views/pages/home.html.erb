<%= render 'shared/messages' %>
<% form_for @user do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>
	
	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :password %>
	<%= f.password_field :password %>
	
	<%= f.label :Confirm_Password %>
	<%= f.password_field :password_confirmation %>
	
	<%= f.submit "Create Account" %>
<% end %>